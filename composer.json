{
    "name": "foorschtbar/tls_icon",
    "type": "roundcube-plugin",
    "description": "Displays a small icon in Roundcube after the subject line that displays the (presumed) encryption state of received mails. ",
    "license": "MIT",
    "keywords": [
        "TLS",
        "SSL",
        "icon",
        "logging"
    ],
    "homepage": "https://github.com/foorschtbar/tls_icon",
    "authors": [
        {
            "name:": "oszto90"
        },
        {
            "name": "Takika"
        },
        {
            "name": "foorschtbar"
        }
    ],
    "version": "2.0.0",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/foorschtbar/tls_icon"
        },
        {
            "type": "composer",
            "url": "https://plugins.roundcube.net"
        }
    ],
    "require": {
        "roundcube/plugin-installer": ">=0.1.6"
    },
    "support": {
        "issues": "https://github.com/foorschtbar/tls_icon/-/issues",
        "source": "https://github.com/foorschtbar/tls_icon"
    },
    "extra": {
        "roundcube": {
            "persistent-files": [
                "config.inc.php"
            ]
        }
    }
}